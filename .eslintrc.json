{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "import/prefer-default-export": "warn",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "consistent-return": "off",
    "no-throw-literal": "off"
  },
  "env": {
    "jest": true
  }
}
